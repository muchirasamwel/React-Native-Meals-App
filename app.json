{
  "expo": {
    "name": "React-Native-Meals-App",
    "slug": "React-Native-Meals-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/burger.png",
    "splash": {
      "image": "./assets/burger.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
